<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="DeleteEditTextView">
        <attr name="icon" format="reference" />
        <attr name="text" format="string" />
        <attr name="hint" format="reference" />
        <attr name="text_size" format="dimension" />
        <attr name="inputType" format="enum">
            <enum name="text" value="1" />
            <enum name="pass" value="2" />
        </attr>
    </declare-styleable>

    <declare-styleable name="HorizontalNavigation">
        <attr name="widgets_content" format="string" />
    </declare-styleable>

    <declare-styleable name="AutoTextView">
        <attr name="text_sizes" format="dimension" />
    </declare-styleable>
</resources>